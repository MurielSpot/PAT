难度：0
*/

#include<cstdio>
#include<cstring>

int main(){
	char num[4];
	scanf("%s",num);
	int n=strlen(num);
		if(n==3){
			for(int i=0;i<num[0]-'0';i++){
				printf("B");
			}
			for(int i=0;i<num[1]-'0';i++){
				printf("S");
			}
			for(int i=1;i<=num[2]-'0';i++){
				printf("%d",i);
			}
		}else if(n==2){
			for(int i=0;i<num[0]-'0';i++){
				printf("S");
			}
			for(int i=1;i<=num[1]-'0';i++){
				printf("%d",i);
			}
		}else if(n==1){
			for(int i=1;i<=num[0]-'0';i++){
				printf("%d",i);
			}
		}
	return 0;
}
